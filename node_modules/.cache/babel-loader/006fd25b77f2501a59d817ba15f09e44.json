ref=\"queryForm\" class=\"demo-form-inline\" v-if=\"showSearch\">\n          <el-form-item label=\"账号\">\n            <el-input v-model=\"query.account\" ></el-input>\n          </el-form-item>\n          <el-form-item label=\"状态\">\n            <el-select v-model=\"query.status\" clearable>\n              <el-option value=\"1\" label=\"启用\"></el-option>\n              <el-option value=\"2\" label=\"禁用\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"时间\">\n              <el-date-picker\n                  v-model=\"query.date\"\n                  type=\"daterange\"\n                  range-separator=\"至\"\n                  start-placeholder=\"开始日期\"\n                  end-placeholder=\"结束日期\"\n                  value-format=\"yyyy-MM-dd\"\n              />\n          </el-form-item>\n          <el-form-item>\n              <el-button type=\"primary\" icon=\"el-icon-search\" @click='search()'>搜索</el-button>\n              <el-button @click='rest()'>重置</el-button>\n          </el-form-item>\n        </el-form>\n        <el-row :gutter=\"10\" class=\"mb8\">\n          <el-col :span=\"1.5\">\n            <el-button\n              type=\"primary\"\n              plain\n              icon=\"el-icon-plus\"\n              size=\"mini\"\n              @click=\"add\"\n              v-auth=\"['user:create']\"\n            >新增</el-button>\n          </el-col>\n          <el-col :span=\"1.5\">\n            <el-button\n              type=\"danger\"\n              plain\n              icon=\"el-icon-delete\"\n              size=\"mini\"\n              @click=\"delAll\"\n              v-auth=\"['user:delete']\"\n            >删除</el-button>\n          </el-col>\n          <el-col :span=\"1.5\">\n            <el-button\n              type=\"warning\"\n              plain\n              icon=\"el-icon-download\"\n              size=\"mini\"\n              @click=\"handleExport\"\n              v-auth=\"['user:export']\"\n            >导出</el-button>\n          </el-col>\n          <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\n        </el-row>\n  \n        <el-table\n            :data=\"list\"\n            row-key=\"id\"\n            ref=\"dragTable\"\n            v-fixed-scroll\n          >\n            <!-- 多选 -->\n            <el-table-column\n              type=\"selection\"\n            ></el-table-column>\n            <el-table-column\n              label=\"账号\"\n              prop=\"account\"\n            >\n            </el-table-column>\n            <el-table-column\n              prop=\"status\"\n              label=\"状态\">\n              <template slot-scope=\"scope\"  v-if=\"scope.row.id !== 1\">\n                <el-switch\n                  :active-value=\"1\"\n                  :inactive-value=\"2\"\n                  v-model=\"scope.row.status\"\n                  v-auth=\"'user:status'\"\n                  v-if=\"userInfo.id != scope.row.id\"\n                  @change=\"(e) => status(e, scope.row.id, scope.$index)\"\n                >\n                </el-switch>\n              </template>\n            </el-table-column>\n            <el-table-column\n              prop=\"created_at\"\n              label=\"创建时间\">\n            </el-table-column>\n            <el-table-column\n              label=\"操作\"\n              fixed=\"right\"\n            >\n              <template slot-scope=\"scope\"  v-if=\"scope.row.id !== 1\">\n                <el-link\n                  type=\"primary\"\n                  icon=\"el-icon-edit\"\n                  :underline=\"false\"\n                  v-auth=\"'user:add'\"\n                  @click=\"edit(scope.$index, scope.row)\"\n                  v-if=\"userInfo.id != scope.row.id\"\n                >编辑</el-link>\n                <el-link\n                  type=\"primary\"\n                  icon=\"el-icon-delete\"\n                  :underline=\"false\"\n                  v-auth=\"'user:del'\"\n                  @click=\"del(scope.$index, scope.row)\"\n                  v-if=\"userInfo.id != scope.row.id\"\n                >删除</el-link>\n              </template>\n            </el-table-column>\n          </el-table>\n          <Pagination :total=\"total\" @pageChange=\"pageChange\" />\n      </el-card>\n      <el-dialog\n        :visible.sync=\"dialogFormVisible\"\n        append-to-body\n        :title=\"title\"\n        width=\"600px\"\n      >\n        <div class=\"dialog-content\">\n          <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"80px\">\n            <el-form-item label=\"头像\" prop=\"avatar\">\n              <div @click=\"$refs.pictureRef.dialogVisible = true\">\n                <el-image \n                    style=\"width: 100px; height: 100px\"\n                    :src=\"form.avatar\" >\n                </el-image>\n              </div>\n            </el-form-item>\n            <el-form-item label=\"账号\" prop=\"account\">\n              <el-input v-model=\"form.account\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"密码\" prop=\"password\" >\n              <el-input v-model=\"form.password\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"昵称\" prop=\"nickname\">\n              <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"手机号\" prop=\"mobile\" >\n              <el-input type=\"number\" v-model=\"form.mobile\" autocomplete=\"off\" maxlength=\"11\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"邮箱\" prop=\"email\" >\n              <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n\n            <el-form-item label=\"角色\"  prop=\"role\">\n              <el-select v-model=\"form.role\" multiple placeholder=\"请选择\">\n                <el-option\n                v-for=\"item in form.roles\"\n                :key=\"item.id\"\n                :label=\"item.name\"\n                :value=\"item.id\">\n              </el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"状态\" prop=\"status\" @change=\"$forceUpdate()\">\n              <el-radio-group v-model=\"form.status\">\n                <el-radio :label=\"1\">正常</el-radio>\n                <el-radio :label=\"2\">禁用</el-radio>\n              </el-radio-group>\n            </el-form-item>\n            <el-form-item label=\"备注\" prop=\"desc\" >\n              <el-input\n                type=\"textarea\"\n                :autosize=\"{ minRows: 2, maxRows: 4}\"\n                placeholder=\"请输入备注\"\n                v-model=\"form.desc\">\n              </el-input>\n            </el-form-item>\n          </el-form>\n        </div>\n        <div slot=\"footer\" class=\"dialog-footer\">\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n          <el-button type=\"primary\" v-throttle=\"2000\" @click=\"submit\">确 定</el-button>\n        </div>\n      </el-dialog>\n      <VkPicture ref=\"pictureRef\" :maxCount=\"1\" @selectImage=\"selectImage\" />\n    </div>\n  </template>\n  \n  <script>\n  import { lists, edit, update, changeStatus, create, del } from \"@/api/auth/user\";\n  import { confirm } from \"@/utils/message-box.js\";\n  import { getTableIds } from \"@/utils/index.js\";\n  import { mapState } from \"vuex\"\n  export default {\n    name: \"index\",\n    computed: {\n\t\t\t...mapState('user', [\"userInfo\"]),\n    },\n    data () {\n      return {\n        query:{},\n        list: [],\n        total: 0,\n        title: \"\",\n        form:{},\n        dialogFormVisible: false,\n        showSearch:true,\n        rules: {\n          account: [\n            { required: true, message: '请输入账号', trigger: 'blur' },\n          ],\n          role: [\n            {required: true, message: '请选择角色', trigger: 'change'
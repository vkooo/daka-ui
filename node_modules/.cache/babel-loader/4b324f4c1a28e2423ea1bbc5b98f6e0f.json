atus == 1?'正常':\"禁用\" }}</el-tag>\n                </template>\n            </el-table-column>\n            <el-table-column\n              prop=\"created_at\"\n              label=\"创建时间\">\n            </el-table-column>\n            <el-table-column\n              label=\"操作\"\n              fixed=\"right\"\n            >\n              <template slot-scope=\"scope\">\n                <el-link\n                  type=\"primary\"\n                  icon=\"el-icon-edit\"\n                  :underline=\"false\"\n                  v-auth=\"'member:add'\"\n                  @click=\"edit(scope.$index, scope.row)\"\n                >编辑</el-link>\n                <el-link\n                  type=\"primary\"\n                  icon=\"el-icon-delete\"\n                  :underline=\"false\"\n                  v-auth=\"'member:del'\"\n                  @click=\"del(scope.$index, scope.row)\"\n                >删除</el-link>\n              </template>\n            </el-table-column>\n          </el-table>\n      </el-card>\n      <el-dialog\n        :visible.sync=\"dialogFormVisible\"\n        append-to-body\n        :title=\"title\"\n      >\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\n            <el-form-item label=\"昵称\" prop=\"nickname\">\n                <el-input v-model=\"form.account\" autocomplete=\"off\" />\n            </el-form-item>\n            <el-form-item label=\"真实姓名\" prop=\"realname\" >\n                <el-input v-model=\"form.realname\" autocomplete=\"off\" />\n            </el-form-item>\n\n            <el-form-item label=\"手机号\" prop=\"mobile\" >\n                <el-input type=\"number\" v-model=\"form.mobile\" autocomplete=\"off\" />\n            </el-form-item>\n            <el-form-item label=\"等级\" prop=\"level_id\" >\n                <el-select v-model=\"form.level_id\" placeholder=\"请选择\">\n                    <el-option\n                        v-for=\"item in form.levels\"\n                      
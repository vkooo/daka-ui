item>\n            <el-form-item label=\"支持文件大小\" prop=\"size\">\n                <el-input type=\"number\" v-model=\"form.size\" autocomplete=\"off\"></el-input>\n                <div class=\"s-guidance\">默认单位为MB</div>\n            </el-form-item>  \n                \n            <el-form-item>\n                <el-button type=\"primary\" @click=\"onSubmit\" v-throttle=\"2000\">提交</el-button>\n                <el-button>取消</el-button>\n            </el-form-item>\n        </el-form>\n    </div>\n</template>\n\n<script>\nimport { save, get } from \"@/api/settings\";\n\nexport default {\n    data() {\n        return {\n            form: {\n                enable_thumb: 2\n            },\n            rules: {},\n        };\n    },\n    created(){\n        get(\"upload\", \"default\").then(res=>{\n            if(res.code == 200 && res.data){\n                this.form = res.data\n            }\n        })\n    },\n    methods: {\n        onSubmit() {\n            this.$refs.ruleForm.validate((valid) => {\n                if(valid){\n                    save({\n                        name: \"upload\",\n                        key: \"default\",\n                        data: this.form\n                    }).then(res=>{\n                        if(res.code == 200){\n                            this.$message({\n                                message: \"操作成功\",\n                                type: \"success\"\n                            })\n                        }\n                    })\n                }\n            })\n        }\n    }\n};\n</script>"]}]}
n-search\" @click='search()'>搜索</el-button>\n                <el-button @click='rest()'>重置</el-button>\n            </el-form-item>\n        </el-form>\n        <el-row :gutter=\"10\" class=\"mb8\">\n          <el-col :span=\"1.5\">\n            <el-button\n              type=\"warning\"\n              plain\n              icon=\"el-icon-download\"\n              size=\"mini\"\n              @click=\"handleExport\"\n              v-auth=\"['member:export']\"\n            >导出</el-button>\n          </el-col>\n          <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\n        </el-row>\n  \n        <el-table\n            :data=\"list\"\n            row-key=\"id\"\n            ref=\"dragTable\"\n            v-fixed-scroll\n          >\n            <!-- 多选 -->\n            <el-table-column\n              type=\"selection\"\n            ></el-table-column>\n            <el-table-column\n              label=\"用户\"\n              prop=\"member.email\"\n            />\n            <el-table-column prop=\"status\" label=\"交易金额\" >\n                <template slot-scope=\"scope\">\n                    <span :style=\"'color:' + (scope.row.type == 1?'red':'')\">\n                        {{ scope.row.type == 1? '+': '-' }} {{ scope.row.money }}\n                    </span>\n                </template>\n            </el-table-column>\n            <el-table-column prop=\"before\" label=\"变更前金额\" />\n            <el-table-column prop=\"after\" label=\"变更后金额\" />\n            <el-table-column\n              prop=\"memo\"\n              label=\"交易备注\" />\n            <el-table-column\n              prop=\"created_at\"\n              label=\"交易时间\" />\n          </el-table>\n          <Pagination :total=\"total\" @pageChange=\"pageChange\" />\n      </el-card>\n    </div>\n  </template>\n  \n  <script>\n  import { lists } from \"@/api/finance/capitalFlow\";\n  import { mapState } from \"vuex\"\n  export default {\n    name: \"index\",\n    computed: {\n\t\t...mapState('user', [\"userInfo\"]),\n    },\n    data () {\n      return {\n        query:{},\n        list: [],\n        total: 0,\n        showSearch:true,\n      }\n    },\n    created() {\n      this.rest()\n    },\n    methods:{\n      queryData(){\n        this.query = {\n            keyword: \"\",\n            status: \"\",\n            date: \"\",\n            page: 1,\n        }\n      },\n      getList(){\n        let that = this\n        lists(that.query).then(res => {\n          if (res.code === 200){\n            const {list, total} = res.data;\n            that.list = list\n            that.total = total\n          }\n        }).catch(error => {\n          console.log(error)\n        })\n      },\n      search(){\n        this.getList();\n      },\n      handleExport:function (){\n  \n      },\n      pageChange(page){\n        this.queryData()\n        this.page = page\n        this.getList();\n      },\n      rest:function (){\n        this.queryData()\n        this.getList();\n      },\n    }\n  }\n  </script>\n  \n  <style scoped>\n  </style>\n  "]}]}
>最终余额</el-radio>\n                    </el-radio-group>\n                </el-form-item>\n                \n                <el-form-item label=\"金额\" prop=\"data\">\n                    <el-input-number v-model=\"form.data\" autocomplete=\"off\"  />\n                    <div class=\"s-guidance\">用户当前余额：{{form.money}}</div>\n                </el-form-item>\n            </el-form>\n\n            <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" v-throttle=\"2000\" @click=\"submit\">确 定</el-button>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport { setMoney, edit } from \"@/api/member\";\nimport { mapState } from \"vuex\"\nexport default {\n    props:{\n        memberId: {\n            type: Number,\n            default: 0\n        }\n    },\n    watch:{\n        dialogVisible: {\n            handler(newVal, oldVal){\n                this.form = {}\n                if(newVal){\n                    edit(this.memberId).then(res => {\n                        this.form.data = 0\n                        this.form = res.data\n                    })\n                }\n            },\n            deep: true,\n            immediate: true\n        }\n    },\n    watch:{\n        dialogVisible: {\n            handler(newVal, oldVal){\n                console.log(newVal)\n                if(newVal){\n                    edit(this.memberId).then(res => {\n                        this.form.data = 0\n                        this.form = res.data\n                    })\n                }\n            },\n            deep: true,\n            immediate: true\n        }\n    },\n    computed: {\n\t\t...mapState('user', [\"userInfo\"]),\n    },\n    data () {\n        return {\n            dialogVisible: false,\n            form: {},\n            rules: {\n                status: [\n                    {required: true, message: '请选择状态', trigger: 'change' }\n                ],\n                data: [\n                    {required: true, message: '请输入金额', trigger: 'change' }\n                ], \n            }\n        }\n    },\n    methods:{\n        submit(){\n            this.$refs.ruleForm.validate((valid) => {\n                if (valid) {\n                    setMoney(this.form).then(res => {\n                    this.$message({\n                        message: \"操作成功\",\n                        type: \"success\"\n                    })\n                    this.dialogVisible = false\n                    })\n                } else {\n                    return false;\n                }\n            });\n        }\n    }\n}\n</script>"]}]}